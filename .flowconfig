[ignore]
<PROJECT_ROOT>/node_modules/styled-components/*
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/.*/__tests__/.*
; Ignore stylelint errors
.*/node_modules/stylelint/.*
; Ignore nock errors
.*/node_modules/nock/.*

[include]

[libs]
; The app's global variables are declared here
tools/flow/definitions
styled-components
tools/flow/flow-typed

[options]
emoji=true

module.system=haste
module.system.node.resolve_dirname=src/be
module.system.node.resolve_dirname=src/fe
module.system.node.resolve_dirname=src/fe/styles
module.system.node.resolve_dirname=src/types
module.system.node.resolve_dirname=src

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

munge_underscores=false

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe
suppress_type=$FlowExpectedError

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe\\($\\|[^(]\\|(\\(>=0\\.\\(3[0-3]\\|[1-2][0-9]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue\\((\\(>=0\\.\\(3[0-3]\\|[1-2][0-9]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)?:? #[0-9]+
suppress_comment=\\(.\\|\n\\)*\\$FlowFixedInNextDeploy
suppress_comment=\\(.\\|\n\\)*\\$FlowExpectedError

[version]
^0.49.1
