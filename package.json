{
  "name": "wettemer",
  "version": "1.0.0",
  "description": "wettemer",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./src/internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets env,stage-0 -- ./src/internals/scripts/extract-intl.js",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config src/internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./src/internals/scripts/dependencies.js",
    "start": "nodemon --exec cross-env NODE_ENV=development node src/be",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean && rimraf ./node_modules",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern src/internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "lint:style": "stylelint 'src/**/*.js' --syntax scss",
    "pretest": "npm run test:clean && npm run lint",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:clean": "rimraf ./coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorinSenoner/cas_fee_02.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FlorinSenoner/cas_fee_02/issues"
  },
  "homepage": "https://github.com/FlorinSenoner/cas_fee_02#readme",
  "lint-staged": {
    "*.js": "lint:eslint",
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "be": "./src/be/",
            "fe": "./src/fe/",
            "internals": "./src/internals/",
            "app": "./src/"
          }
        }
      ],
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": true
        }
      ]
    ],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "src"
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "pre-commit": "lint:staged",
  "dllPlugin": {
    "path": "node_modules/wettemer-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css",
      "mongoose"
    ],
    "include": [
      "babel-polyfill",
      "lodash"
    ]
  },
  "dependencies": {
    "apollo-server-express": "^1.3.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "compression": "^1.7.1",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "graphql-tools": "^2.13.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "intl": "^1.2.5",
    "ip": "^1.1.5",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "mongoose": "^4.13.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "sanitize.css": "^5.0.0",
    "styled-components": "^2.2.4",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link-http": "^1.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "circular-dependency-plugin": "^4.2.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.5",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-redux-saga": "^0.6.0",
    "file-loader": "^1.1.5",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.6.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.4.2",
    "jest-cli": "^21.2.1",
    "lint-staged": "^6.0.0",
    "ngrok": "^2.2.24",
    "nodemon": "^1.12.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.0",
    "react-apollo": "^2.0.4",
    "rimraf": "^2.6.2",
    "shelljs": "^0.7.8",
    "style-loader": "^0.19.0",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.4.0",
    "stylelint-processor-styled-components": "^1.2.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  }
}
